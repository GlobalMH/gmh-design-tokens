{
  "Typography": {
    "body": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{fontFamilies.sans}",
        "fontWeight": "{fontWeights.regular}",
        "lineHeight": "{lineHeights.base}",
        "fontSize": "{fontSizes.base}",
        "letterSpacing": "{letterSpacing.default}",
        "paragraphSpacing": "{paragraphSpacing.default}",
        "paragraphIndent": "{paragraphIndent.default}",
        "textCase": "{textCase.none}",
        "textDecoration": "{textDecoration.none}"
      }
    },
    "code": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{fontFamilies.code}",
        "fontWeight": "{fontWeights.regular}",
        "lineHeight": "{lineHeights.base}",
        "fontSize": "{fontSizes.base}",
        "letterSpacing": "{letterSpacing.default}",
        "paragraphSpacing": "{paragraphSpacing.0}",
        "paragraphIndent": "{paragraphIndent.0}",
        "textCase": "{textCase.none}",
        "textDecoration": "{textDecoration.none}"
      }
    }
  },
  "Color": {
    "primary": {
      "$type": "color",
      "$value": {
        "default": "{themes/light.primary.default}",
        "on": "{themes/light.primary.on}",
        "background": "{themes/light.primary.background}"
      }
    },
    "secondary": {
      "$type": "color",
      "$value": {
        "default": "{themes/light.secondary.default}",
        "on": "{themes/light.secondary.on}",
        "background": "{themes/light.secondary.background}"
      }
    },
    "surface": {
      "$type": "color",
      "$value": {
        "dim": "{themes/light.surface.dim}",
        "default": "{themes/light.surface.default}",
        "bright": "{themes/light.surface.bright}"
      }
    }
  },
